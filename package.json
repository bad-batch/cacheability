{
  "name": "cacheability",
  "version": "0.0.2",
  "description": "A utility class for parsing http cache headers.",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/dylanaubrey/cacheability",
  "bugs": {
    "url": "https://github.com/dylanaubrey/cacheability/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dylanaubrey/cacheability"
  },
  "main": "lib/main/index.js",
  "module": "lib/module/index.js",
  "browser": "lib/browser/index.js",
  "keywords": [
    "http",
    "cacheControl",
    "etag",
    "headers",
    "parser"
  ],
  "scripts": {
    "build": "npm run test && npm run transpile",
    "clean": "rimraf lib/*",
    "coverage": "cross-env TS_NODE_PROJECT=\"./tsconfig.test.json\" nyc mocha --opts \"./mocha.opts\"",
    "lint": "tslint --project \"./tsconfig.json\" --fix",
    "publish:pkg": "npm run build && npm publish",
    "test": "npm run lint && npm run type-check && npm run coverage",
    "test:web": "cross-env BABEL_ENV=test:web karma start --single-run --browsers PhantomJS",
    "test:web:debug": "cross-env BABEL_ENV=test:web:debug karma start --no-single-run --browsers Chrome",
    "transpile": "npm run clean && npm run transpile:browser && npm run transpile:main && npm run transpile:module",
    "transpile:browser": "cross-env BABEL_ENV=browser babel src --out-dir lib/browser --copy-files --extensions \".ts\"",
    "transpile:main": "cross-env BABEL_ENV=main babel src --out-dir lib/main --copy-files --extensions \".ts\"",
    "transpile:module": "cross-env BABEL_ENV=module babel src --out-dir lib/module --copy-files --extensions \".ts\"",
    "type-check": "tsc"
  },
  "dependencies": {
    "es6-promise": "^4.1.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.34",
    "@babel/core": "^7.0.0-beta.34",
    "@babel/polyfill": "^7.0.0-beta.34",
    "@babel/preset-env": "^7.0.0-beta.34",
    "@babel/preset-stage-0": "^7.0.0-beta.34",
    "@babel/preset-typescript": "^7.0.0-beta.34",
    "@types/chai": "^4.0.8",
    "@types/es6-promise": "0.0.33",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/lodash": "^4.14.85",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.53",
    "@types/sinon": "^4.0.0",
    "@types/sinon-chai": "^2.7.29",
    "babel-core": "^7.0.0-beta.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "chai": "^4.0.2",
    "codecov": "^3.0.0",
    "cross-env": "^5.0.1",
    "karma": "^1.7.1",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript-preprocessor": "^0.3.1",
    "karma-webpack": "^2.0.6",
    "mocha": "^4.0.1",
    "nyc": "^11.0.3",
    "phantomjs-prebuilt": "^2.1.16",
    "rimraf": "^2.6.1",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.11.0",
    "source-map-support": "^0.5.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "webpack": "^3.10.0"
  }
}
