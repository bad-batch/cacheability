{
  "check-coverage": true,
  "branches": 70,
  "functions": 80,
  "lines": 80,
  "statements": 80,
  "all": true,
  "cache": true,
  "exclude": ["**/*.d.ts"],
  "extension": [".ts"],
  "include": ["src/**/*"],
  "instrument": true,
  "report-dir": "./coverage/node",
  "reporter": ["lcov", "text-summary"],
  "sourceMap": true
}
